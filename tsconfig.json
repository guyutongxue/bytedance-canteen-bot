{
  "compilerOptions": {
    "target": "ESNext",
    "module": "Preserve",
    "noEmit": true,

    "moduleDetection": "force",
    "verbatimModuleSyntax": true,
    "jsx": "react-jsx",

    "strict": true,
    "noFallthroughCasesInSwitch": true,
    "forceConsistentCasingInFileNames": true,

    "types": ["temporal-spec", "bun"]
  }
}